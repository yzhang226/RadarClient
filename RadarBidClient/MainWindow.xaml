<Window x:Class="RadarBidClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RadarBidClient"
        mc:Ignorable="d"
        Title="雷达竞拍" Height="536.018" Width="496.102" Icon="icon-radar2.ico">
    <Canvas HorizontalAlignment="Left" Height="479" Margin="19,10,0,0" VerticalAlignment="Top" Width="464">
        <Label x:Name="label" Content="坐标" Canvas.Left="28" Canvas.Top="27"/>
        <TextBox x:Name="x1" Height="23" Canvas.Left="94" TextWrapping="Wrap" Text="200" Canvas.Top="27" Width="120" TextChanged="textBox_TextChanged_1"/>
        <TextBox x:Name="y1" Height="23" Canvas.Left="94" TextWrapping="Wrap" Text="100" Canvas.Top="69" Width="120"/>
        <Button x:Name="button" Content="移动鼠标" Canvas.Left="261" Canvas.Top="26" Width="75" Click="button_Click"/>
        <Button x:Name="button1" Content="移动鼠标后按下左键后输入文本" Canvas.Left="261" Canvas.Top="151" Width="177" Height="35" Click="button1_Click"/>
        <TextBox x:Name="targetText" Height="23" Canvas.Left="94" TextWrapping="Wrap" Text="Test-English-Text" Canvas.Top="151" Width="120"/>
        <Label x:Name="label1" Content="目标文本" Canvas.Left="28" Canvas.Top="151"/>
        <Label x:Name="label2" Content="高/宽" Canvas.Left="28" Canvas.Top="245"/>
        <TextBox x:Name="h1" Height="23" Canvas.Left="94" TextWrapping="Wrap" Text="1800" Canvas.Top="247" Width="120" TextChanged="h1_TextChanged"/>
        <TextBox x:Name="w1" Height="23" Canvas.Left="94" TextWrapping="Wrap" Text="1900" Canvas.Top="303" Width="120" TextChanged="w1_TextChanged"/>
        <Button x:Name="button2" Content="截图" Canvas.Left="261" Canvas.Top="247" Width="75" Click="button2_Click"/>
        <Button x:Name="button3" Content="获取机器码" Canvas.Left="363" Canvas.Top="248" Width="75" Click="button3_Click"/>
        <Button x:Name="button4" Content="关闭IE然后打开竞拍页面" Canvas.Left="261" Canvas.Top="309" Width="152" Click="button4_Click" Height="17"/>
        <TextBox x:Name="processTitle" Height="23" Canvas.Left="94" TextWrapping="Wrap" Text="客车" Canvas.Top="411" Width="120"/>
        <TextBox x:Name="processClassName" Height="23" Canvas.Left="94" TextWrapping="Wrap" Canvas.Top="362" Width="120" TextChanged="processClassName_TextChanged" Text="IEFrame"/>
        <Button x:Name="mockStart" Content="模拟首页-登录-第一阶段" Canvas.Left="261" Canvas.Top="348" Width="148" Height="17" Click="mockStart_Click"/>
        <Button x:Name="button6" Content="识别图片中的文字" Canvas.Left="261" Canvas.Top="444" Width="136" Height="23" Click="button6_Click"/>
        <Button x:Name="mockLogin" Content="模拟第二阶段" Canvas.Left="261" Canvas.Top="397" Width="75" Click="mockPhase2_Click"/>
    </Canvas>
</Window>
